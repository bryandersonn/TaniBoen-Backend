<body>
    <div class="navbar">
        <div class="left-section">
            <a asp-controller="Home" asp-action="Index">
                <img src="/assets/logo taniboen remove bg2.png" alt="logo taniboen">
            </a>
            TaniBoen
        </div>
        <div class="right-section">
            <ul class="menu">
                <li class="menu-choice">
                    <a class="menu-link" asp-controller="Home" asp-action="Index">Home</a>
                </li>
                <li class="menu-choice">
                    <a class="menu-link" asp-controller="Login" asp-action="UserLogin">Logout</a>
                </li>
                <li class="menu-choice">
                    <a class="menu-link" asp-controller="ShoppingCart" asp-action="ShoppingCart">
                        <img src="/assets/shopping-cart-removebg-preview.png" alt="shopping-cart">
                    </a>
                </li>
                <li class="menu-choice">
                    <a class="menu-link" asp-controller="Profile" asp-action="Profile">
                        <img src="/assets/profile.png" alt="profile">
                    </a>
                </li>
                <li class="menu-choice">
                    <a class="menu-link" asp-controller="Chat" asp-action="Chat">
                        <img src="/assets/chat-page.png" alt="chat">
                    </a>
                </li>
            </ul>
            <div class="hamburger">
                <span class="bar"></span>
                <span class="bar"></span>
                <span class="bar"></span>
            </div>
        </div>
    </div>

    <div class="main">
        <div class="product">
            <div class="product-img">
                <img src="/assets/kangkung.jpg" class="vege-img">
            </div>

            <div class="product-information">
                <div class="product-title">
                    Sayur Kangkung Segar, Murah Makassar
                </div>

                <div class="product-selling">
                    <div class="product-rating">
                        <div class="starinfo">4.0</div>
                        <span class="fa fa-star checked"></span>
                        <span class="fa fa-star checked"></span>
                        <span class="fa fa-star checked"></span>
                        <span class="fa fa-star checked"></span>
                        <span class="fa fa-star unchecked"></span>
                    </div>

                    <div class="product-total-review">
                        <span class="numbertotal">100</span>
                        <span class="labeltotal">Penilaian</span>
                    </div>

                    <div class="product-sold">
                        <span class="numbertotal">500</span>
                        <span class="labeltotal">Terjual</span>
                    </div>
                </div>

                <div class="product-price">
                    Rp12.000
                </div>
                <div class="product-shipping">
                    Pengiriman
                    <i class="fa-solid fa-truck"></i>
                </div>

                <div class="product-destination">
                    Dikirim ke:  <span class="destination-delivery">Kota Jakarta Barat</span>
                </div>

                <div class="product-deliverycost">
                    Ongkos Kirim:
                    <div class="dropdown">
                        <button id="selectedDelivery">Pilih Ongkir</button>
                        <div class="deliverytype">
                            <button onclick="showText(this)" class="reguler">Reguler <span class="deliveryprice">| Rp5.000</span></button>
                            <button onclick="showText(this)" class="today">Today <span class="deliveryprice">| Rp10.000</span></button>
                            <button onclick="showText(this)" class="instant">Instant <span class="deliveryprice">| Rp20.000</span></button>
                        </div>
                    </div>
                </div>

                <div class="result" id="result"></div>

                <div class="product-quantity">
                    Kuantitas:
                    <button id="decreaseButton">
                        <i class="fa-solid fa-minus"></i>
                    </button>

                    <label id="countLabel">0</label>

                    <button id="increaseButton">
                        <i class="fa-solid fa-plus"></i>
                    </button>

                    <label class="product-avail">
                        50 tersisa
                    </label>
                </div>

                <div class="product-confirmation">
                    <div class="cart">
                        <form action="AddToCart" method="post" autocomplete="off">
                            <button type="submit">
                                <img src="/assets/shopping-cart-removebg-preview.png" alt="shopping-cart">
                                Masukkan Keranjang
                            </button>
                        </form>
                    </div>
                    <div class="buy">
                        <a asp-controller="Payment" asp-action="Payment"><button>Beli Sekarang</button></a>
                    </div>
                </div>
            </div>
        </div>

        <div class="product-seller-information">
            <div class="left-seller-container">
                <div class="seller">
                    <img src="/assets/mun_and_co-removebg-preview.png" alt="seller-logo">
                </div>

                <div class="seller-bio-container">
                    <div class="seller-title">
                        Mun & Co.
                    </div>

                    <div class="seller-online">
